         alert(\"password don't match\");\n            return;\n        }\n\n        try {\n            const {user} = await auth.createUserWithEmailAndPassword(email, password);\n\n            await createUserProfileDocument(user, {displayName});\n\n            this.setState({\n                displayName: '',\n                email: '',\n                password: '',\n                confirmPassword: ''\n            });\n        } catch (error) {\n            console.error(error);\n        }\n    };\n\n    handleChange = event => {\n        const {name, value} = event.target;\n\n        this.setState({[name]: value});\n    };\n\n    render() {\n        const {displayName, email, password, confirmPassword} = this.state;\n        return(\n            <div className=\"sign-up\">\n                <h2 className='title'>I do not have an account</h2>\n                <span>Sign up with your email and password</span>\n                <form className='sign-up-form' onSubmit={this.handleSubmit}>\n                    <FormInput\n                        type='text'\n                        name=\"displayName\"\n                        value={displayName}\n                        onChange={this.handleChange}\n                        label='Display Name'\n                        required\n                    />\n                    <FormInput\n                        type='email'\n                        name=\"email\"\n                        value={email}\n                        onChange={this.handleChange}\n                        label='Email'\n                        required\n                    />\n                    <FormInput\n                        type='password'\n                        name=\"password\"\n                        value={password}\n                        onChange={this.handleChange}\n                        label='Password'\n                        required\n                    />\n                    <FormInput\n                        type='password'\n                        name=\"confirmPassword\"\n                        value={confirmPassword}\n                        onChange={this.handleChange}\n                        label='Confirm Password'\n                        required\n                    />\n                    <CustomButton type='submit'>SIGN UP</CustomButton>\n\n                </form>\n            </div>\n        )\n    }\n}\n\nexport default SignUp;\n","/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/pages/shop/shop.data.js",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/user/user.types.js",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/user/user.actions.js",[],["149","150"],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/components/cart-icon/cart-icon.components.jsx",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/components/cart-dropdown/cart-dropdown.component.jsx",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/cart/cart.types.js",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/cart/cart.actions.js",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/root-reducer.js",[],["151","152"],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/cart/cart.reducer.js",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/user/user.reducer.js",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/store.js",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/cart/cart.utils.js",["153","154","155","156","157","158"],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/redux/cart/cart.selectors.js",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/components/cart-item/cart-item.components.jsx",[],"/Users/bgriner/developer/education/react-ztm/crwn-clothing/src/pages/checkout/checkout.component.jsx",[],["159","160"],"/Users/bgr